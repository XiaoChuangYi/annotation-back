<settings>
  <servers>
    <server>
      <id>malgo</id>
      <username>${env.MAVEN_REPO_USER}</username>
      <password>${env.MAVEN_REPO_PASS}</password>
    </server>
    <server>
      <id>malgo-algorithm</id>
      <username>${env.MAVEN_REPO_USER}</username>
      <password>${env.MAVEN_REPO_PASS}</password>
    </server>
  </servers>
  <profiles>
    <profile>
      <id>malgo</id>
      <repositories>
        <repository>
          <id>central</id>
          <url>${env.MAVEN_CENTRAL_REPO}</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
        <repository>
          <id>malgo-algorithm</id>
          <url>http://nexus.malgo.cn:8081/repository/algorithm-snapshots/</url>
          <releases><enabled>false</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
        <repository>
          <id>malgo</id>
          <url>http://nexus.malgo.cn:8081/repository/maven-public/</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
      </repositories>
     <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>${env.MAVEN_CENTRAL_REPO}</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </pluginRepository>
       <pluginRepository>
         <id>malgo</id>
         <url>http://nexus.malgo.cn:8081/repository/maven-public/</url>
         <releases><enabled>true</enabled></releases>
         <snapshots><enabled>true</enabled></snapshots>
       </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>malgo</activeProfile>
  </activeProfiles>
</settings>
